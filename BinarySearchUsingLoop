#include <iostream>
using namespace std;

Bool search(int arr[], int n) {

	int first = 0;int  mid = 2;
	while (first < mid) {
		if (arr[mid] == 31)
			return true;

		if (arr[mid]>31) {
			first = 0;
			mid = mid - 1;
		}
		else {
			first = mid + 1;
			mid = 5;
		}
			
	}
	return false;
}

int main()
{
	int x[5] = { 1, 2, 3, 4, 5};
	if (search(x, 5))
	{
		cout << "key found" << endl;
	}
	else
		cout << "key not found" << endl;
}
